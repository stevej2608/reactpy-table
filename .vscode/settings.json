{
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.exclude": [
        ".venv",
        ".devcontainer",
        "tmp"
    ],
    "cSpell.words": [
        "ilike",
        "sessionmaker",
        "sqlalchemy",
        "sqlmodel"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        ".devcontainer",
    ]
}